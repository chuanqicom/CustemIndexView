#import "QueryResultModel.h"

@implementation QueryResultModel

- (float)monthAmount
{
    _monthAmount = self.personalMonthPayAmount + self.companyMonthPayAmount;
    return _monthAmount;
}

- (NSString *)queryTime
{
    _queryTime = [TimeUtils getSystemTimeStringWithFormat:@"yyyy-MM-dd"];
    return _queryTime;
}

- (float)TotalAmount
{
    return _totalAmount;
}

@end

